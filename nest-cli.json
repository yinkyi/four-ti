{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/todo-app/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/todo-app/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/todo-app",
  "projects": {
    "todo-app": {
      "type": "application",
      "root": "apps/todo-app",
      "entryFile": "main",
      "sourceRoot": "apps/todo-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/todo-app/tsconfig.app.json"
      }
    },
    "repository": {
      "type": "library",
      "root": "libs/repository",
      "entryFile": "index",
      "sourceRoot": "libs/repository/src",
      "compilerOptions": {
        "tsConfigPath": "libs/repository/tsconfig.lib.json"
      }
    },
    "four-ti-log": {
      "type": "library",
      "root": "libs/four-ti-log",
      "entryFile": "index",
      "sourceRoot": "libs/four-ti-log/src",
      "compilerOptions": {
        "tsConfigPath": "libs/four-ti-log/tsconfig.lib.json"
      }
    },
    "auth0": {
      "type": "library",
      "root": "libs/auth0",
      "entryFile": "index",
      "sourceRoot": "libs/auth0/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth0/tsconfig.lib.json"
      }
    }
  }
}